<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<t:layout 
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" 
	xmlns:p="tapestry:parameter"
	xmlns:roomeeting="tapestry-library:roomeeting">


<style type="text/css">

table.t-data-grid {
    border-collapse: collapse;
}
table.t-data-grid tbody, table.t-data-grid tr, table.t-data-grid td {
    border-left: 1px solid #C29A71;
}
table.t-data-grid tr:HOVER {
	background-color: #C29A71;
}
table.t-data-grid tr.selected {
	background-color: lightblue;
}
.t-data-grid thead th {
	background-color: #BA3E01;
}
.t-data-grid thead a {
	text-decoration: none;
}
</style>

	<t:grid t:id="grid1" id="grid1" source="users" row="current"
	t:mixins="roomeeting/ActionableGridRow" t:cell="id">
	</t:grid>
	
</t:layout>
